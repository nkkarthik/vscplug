{
  "name": "vscode-shell-executor",
  "displayName": "VSCode Shell Executor",
  "description": "An extension to execute selected lines in the shell using a keyboard shortcut.",
  "version": "0.1.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:vscode-shell-executor.activate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-shell-executor.executeSelectedLine",
        "title": "Execute Selected Line"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-shell-executor.executeSelectedLine",
        "key": "ctrl+j",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "vscode": "^1.1.36",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  }
}